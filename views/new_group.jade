extends layout

block append scripts
    script(src='/javascripts/libs/jquery-1.9.0.min.js')

block content
    div#wrapper
        div.container-fluid
            div.row-fluid.segment
                img.centered(src='/images/banner.png')
            div.row-fluid.segment
                img.centered#schema(src='/images/schema.png')
            div.row-fluid.segment
                div.span6
                    div.iframe-container
                        iframe.maps(frameborder="0",scrolling="no",marginheight="0",marginwidth="0",src="https://maps.google.com/maps/ms?msa=0&amp;msid=215510472582204168123.0004d4965e05c60ba5805&amp;ie=UTF8&amp;t=m&amp;ll=46.286224,1.889648&amp;spn=10.630383,18.676758&amp;z=5&amp;output=embed")
                            small 
                                span View 
                                a(href="https://maps.google.com/maps/ms?ll=46.7248,3.186035&amp;spn=10.575586,23.24707&amp;t=m&amp;z=6&amp;msa=0&amp;msid=215510472582204168123.0004d4965e05c60ba5805&amp;source=embed",style="color:#0000FF;text-align:left") meeet-up-marketing-dev-design
                                span  in a larger map
                div.span6
                    div.well.form-container
                        form.form-horizontal(action="http://ekito.us6.list-manage2.com/subscribe/post?u=1dfdf26147721a052cf940e37&amp;id=63fcae47fc",method="post")
                            legend Inscrivez-vous à la mailing list!
                            div.control-group
                                label.control-label(for="mce-EMAIL") E-mail
                                div.controls
                                    input(type="email",value="",name="EMAIL",id="mce-EMAIL",placeholder="",required)
                            div.control-group
                                label.control-label(for="mce-FNAME") Prénom
                                div.controls
                                    input(type="text",value="",name="FNAME",id="mce-FNAME",placeholder="",required)
                            div.control-group
                                label.control-label(for="mce-LNAME") Nom
                                div.controls
                                    input(type="text",value="",name="LNAME",id="mce-LNAME",placeholder="",required)
                            div.control-group
                                label.control-label(for="mce-MMERGE3") Groupe
                                div.controls
                                    select(name="MMERGE3",id="mce-MMERGE3",required)
                                        option(value="")
                                        option(value="Marketing") Marketing
                                        option(value="Développeur") Développeur
                                        option(value="Designer") Designer
                            div.control-group
                                div.controls
                                    button.btn.btn-primary Envoyer
