                            legend Inscrivez-vous !
                            div.control-group
                                label.control-label(for="mce-FNAME") Je m'appelle :
                                div.controls.dual
                                    input.green(type="text",value="",name="FNAME",id="mce-FNAME",placeholder="Prénom",required)
                                    input.aqua(type="text",value="",name="LNAME",id="mce-LNAME",placeholder="Nom",required)
                            div.control-group
                                label.control-label(for="mce-EMAIL") Mon adresse email :
                                div.controls
                                    input.purple(type="email",value="",name="EMAIL",id="mce-EMAIL",placeholder="email",required)

                            div.control-group
                                label.control-label(for="mce-GROUP") Je suis un :
                                div.controls
                                    select(name="GROUP",id="mce-GROUP",required)
                                        option(value="")
                                        option(value="Marketing") Marketing
                                        option(value="Développeur") Développeur
                                        option(value="Designer") Designer
                            div.control-group
                                label.control-label(for="mce-CITY") à
                                div.controls
                                    select(name="CITY",id="mce-CITY",required)
                                        option(value="")
                                        option(value="Lyon") Lyon
                                        option(value="Paris") Paris
                                        option(value="Toulouse") Toulouse
                            div.control-group
                                div.controls
                                    button.btn.btn-primary.btn-large S'inscrire
