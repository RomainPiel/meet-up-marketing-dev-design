extends layout

block append scripts
    script(src='/javascripts/libs/jquery-1.9.0.min.js')
    script(src='/javascripts/jQueryRotateCompressed.2.2.js')

    script
        !function(d,s,id){var js,fjs=d.getElementsByTagName(s)[0];if(!d.getElementById(id)){js=d.createElement(s);js.id=id;js.src="//platform.twitter.com/widgets.js";fjs.parentNode.insertBefore(js,fjs);}}(document,"script","twitter-wjs");

    script
        $(document).ready(function() {
            console.log("test");
            $("#schema_rounds").rotate({ 
               bind: { 
                    mouseover : function() { 
                        $(this).rotate({animateTo:360})
                    },
                    mouseout : function() { 
                        $(this).rotate({animateTo:0})
                    }
                 } 
               
            });
        });


block content
    div#wrapper
        div.container-fluid
            div.row-fluid.segment
                div.centered
                    img(src='/images/banner.png')
            div.row-fluid.segment
                div.centered
                    img#schema(src='/images/schema.png')
                    img#schema_rounds(src='/images/schema_rounds.png')

            div.row-fluid.segment
                div.span6
                    div.iframe-container
                        iframe.maps(frameborder="0",scrolling="no",marginheight="0",marginwidth="0",src="https://maps.google.com/maps/ms?msa=0&amp;msid=215510472582204168123.0004d4965e05c60ba5805&amp;ie=UTF8&amp;t=m&amp;ll=46.286224,1.889648&amp;spn=10.630383,18.676758&amp;z=5&amp;output=embed")
                            small 
                                span View 
                                a(href="https://maps.google.com/maps/ms?ll=46.7248,3.186035&amp;spn=10.575586,23.24707&amp;t=m&amp;z=6&amp;msa=0&amp;msid=215510472582204168123.0004d4965e05c60ba5805&amp;source=embed",style="color:#0000FF;text-align:left") meeet-up-marketing-dev-design
                                span  in a larger map
                    center
